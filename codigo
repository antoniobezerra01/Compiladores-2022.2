begin{

    dproc soma(Integer a, Integer b){
        Integer x = a + b;
        println(x);
    };

    dfunc Integer maiorOuIgual(Integer c, Integer d){
        Integer resultado = 0;
        if(c > d){
            resultado = c;
        }
        else{
            resultado = d;
        }
        println(c + d);
        return 5;
    };

    dfunc Boolean media(Integer n1, Integer n2){
        Integer somatorio = n1 + n2;
        Integer result = somatorio / 2;
        Boolean aprovacao = False;

        if(result >= 7){
            aprovacao = True;
        }

        return aprovacao;
    };

    dfunc Integer retornoFuncao (Integer c1, Integer d1){
        return c1;
    };

    Integer e = 10;
    Integer f = 8;

    Boolean teste1 = cfunc media(e, f);

    while(e > f){
        if(f <= 8){
            Boolean teste10 = cfunc media(e, f);
            continue;
        }
        else{
            Integer dobro = 2  * f;
        }
        
        Boolean teste2 = cfunc media(e, f);
        f = f + 1;
    }

    Integer i = 10;
    Integer j = 5;

    cproc soma(i, j);
    Integer maior = cfunc maiorOuIgual(i, j);
    Integer funcao = cfunc retornoFuncao(i, j);
    println(cfunc media(i, j));

}end
